<p id="notice"><%= notice %></p>

<h1>Vehicles</h1>

<table>
  <thead>
    <tr>
      <th>V make</th>
      <th>V model</th>
      <th>Mileage</th>
      <th>Price</th>
      <th>Year</th>
      <th>Dealership</th>
      <th>Comments</th>
      <th>Rating</th>
      <th>Zipcode</th>
      <th>Image url</th>
      <th>Video url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vehicles.each do |vehicle| %>
      <tr>
        <td><%= vehicle.v_make %></td>
        <td><%= vehicle.v_model %></td>
        <td><%= vehicle.mileage %></td>
        <td><%= vehicle.price %></td>
        <td><%= vehicle.year %></td>
        <td><%= vehicle.dealership %></td>
        <td><%= vehicle.comments %></td>
        <td><%= vehicle.rating %></td>
        <td><%= vehicle.zipcode %></td>
        <td><%= vehicle.image_url %></td>
        <td><%= vehicle.video_url %></td>
        <td><%= link_to 'Show', vehicle %></td>
        <td><%= link_to 'Edit', edit_vehicle_path(vehicle) %></td>
        <td><%= link_to 'Destroy', vehicle, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vehicle', new_vehicle_path %>
